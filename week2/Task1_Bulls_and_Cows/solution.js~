function getRandomInt(min, max) {
	return Math.floor(Math.random() * (max - min)) + min;
}

var prompt = require('prompt');
prompt.start();
prompt.get(['number'], function (err, result) {
	var isEqual = false;
    	if(result == getRandomInt(1000,9999)){
    		//console.log(true);
		isEqual = true;
	}
	else
	{
    		//console.log(false);
	}
	console.log(isEqual);
	
    
  });
